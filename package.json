    "@emotion/styled": "^11.9.3",
    "@mui/lab": "5.0.0-alpha.93",
    "@mui/material": "^5.8.6",
    "@tanstack/react-query": "^4.1.3",
    "@tanstack/react-query-devtools": "^4.2.1",
    "axios": "^0.27.2",
    "framer-motion": "^6.3.16",
    "lodash": "^4.17.19",
    "next": "^12.2.0",
    "notistack": "^2.0.5",
    "react": "^18.2.0",
    "react-calendar": "^3.7.0",
    "react-confetti": "^6.1.0",
    "react-dom": "^18.2.0",
    "react-fast-marquee": "^1.3.5",
